\n        // ãƒŸãƒ³ãƒˆå¯èƒ½ãªå…¨ NFT ã‚­ãƒ£ãƒ©ã‚¯ã‚¿ãƒ¼ ã‚’ã‚³ãƒ³ãƒˆãƒ©ã‚¯ãƒˆã‚’ã‹ã‚‰å‘¼ã³å‡ºã—ã¾ã™ã€‚\n        const charactersTxn = await gameContract.getAllDefaultCharacters();\n\n        console.log(\"charactersTxn:\", charactersTxn);\n\n        // ã™ã¹ã¦ã®NFTã‚­ãƒ£ãƒ©ã‚¯ã‚¿ãƒ¼ã®ãƒ‡ãƒ¼ã‚¿ã‚’å¤‰æ›ã—ã¾ã™ã€‚\n        const characters = charactersTxn.map((characterData) =>\n          transformCharacterData(characterData)\n        );\n\n        // ãƒŸãƒ³ãƒˆå¯èƒ½ãªã™ã¹ã¦ã®NFTã‚­ãƒ£ãƒ©ã‚¯ã‚¿ãƒ¼ã®çŠ¶æ…‹ã‚’è¨­å®šã—ã¾ã™ã€‚\n        setCharacters(characters);\n      } catch (error) {\n        console.error(\"Something went wrong fetching characters:\", error);\n      }\n    };\n\n    // ã‚¤ãƒ™ãƒ³ãƒˆã‚’å—ä¿¡ã—ãŸã¨ãã«èµ·å‹•ã™ã‚‹ã‚³ãƒ¼ãƒ«ãƒãƒƒã‚¯ãƒ¡ã‚½ãƒƒãƒ‰ onCharacterMint ã‚’è¿½åŠ ã—ã¾ã™ã€‚\n    const onCharacterMint = async (sender, tokenId, characterIndex) => {\n      console.log(\n        `CharacterNFTMinted - sender: ${sender} tokenId: ${tokenId.toNumber()} characterIndex: ${characterIndex.toNumber()}`\n      );\n      // NFT ã‚­ãƒ£ãƒ©ã‚¯ã‚¿ãƒ¼ãŒ Mint ã•ã‚ŒãŸã‚‰ã€ã‚³ãƒ³ãƒˆãƒ©ã‚¯ãƒˆã‹ã‚‰ãƒ¡ã‚¿ãƒ‡ãƒ¼ã‚¿ã‚’å—ã‘å–ã‚Šã€ã‚¢ãƒªãƒ¼ãƒŠï¼ˆãƒœã‚¹ã¨ã®ãƒãƒˆãƒ«ãƒ•ã‚£ãƒ¼ãƒ«ãƒ‰ï¼‰ã«ç§»å‹•ã™ã‚‹ãŸã‚ã®çŠ¶æ…‹ã«è¨­å®šã—ã¾ã™ã€‚\n      if (gameContract) {\n        const characterNFT = await gameContract.checkIfUserHasNFT();\n        console.log(\"CharacterNFT: \", characterNFT);\n        setCharacterNFT(transformCharacterData(characterNFT));\n      }\n    };\n\n    if (gameContract) {\n      getCharacters();\n      // ãƒªã‚¹ãƒŠãƒ¼ã®è¨­å®šï¼šNFT ã‚­ãƒ£ãƒ©ã‚¯ã‚¿ãƒ¼ãŒ Mint ã•ã‚ŒãŸé€šçŸ¥ã‚’å—ã‘å–ã‚Šã¾ã™ã€‚\n      gameContract.on(\"CharacterNFTMinted\", onCharacterMint);\n    }\n\n    return () => {\n      // ã‚³ãƒ³ãƒãƒ¼ãƒãƒ³ãƒˆãŒãƒã‚¦ãƒ³ãƒˆã•ã‚ŒãŸã‚‰ã€ãƒªã‚¹ãƒŠãƒ¼ã‚’åœæ­¢ã™ã‚‹ã€‚\n\n      if (gameContract) {\n        gameContract.off(\"CharacterNFTMinted\", onCharacterMint);\n      }\n    };\n  }, [gameContract]);\n\n  // index.js\n  // NFT ã‚­ãƒ£ãƒ©ã‚¯ã‚¿ãƒ¼ã‚’ãƒ•ãƒ­ãƒ³ãƒˆã‚¨ãƒ³ãƒ‰ã«ãƒ¬ãƒ³ãƒ€ãƒª